{
  "private": true,
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:remix && npm run build:fastly",
    "build:remix": "remix build",
    "prebuild:fastly": "npx @fastly/compute-js-static-publish --build-static",
    "build:fastly": "js-compute-runtime ./src/index.js ./bin/main.wasm",
    "deploy": "fastly compute publish",
    "dev:remix": "remix watch",
    "dev:fastly": "fastly compute serve --watch",
    "dev": "run-p \"dev:*\"",
    "start": "fastly compute serve"
  },
  "dependencies": {
    "@fastly/remix-server-adapter": "^1.0.0",
    "@fastly/remix-server-runtime": "^1.0.0",
    "@remix-run/react": "^1.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@fastly/compute-js-static-publish": "^4.0.0",
    "@fastly/js-compute": "^1.4.0",
    "@remix-run/dev": "^1.17.0",
    "@remix-run/eslint-config": "^1.17.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.23.1",
    "events": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16.13"
  }
}
